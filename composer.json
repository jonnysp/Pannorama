{
    "name": "jonnysp/pannorama",
    "type": "contao-bundle",
    "description": "Adds Pannorama functionality to Contao 4",
    "license": "LGPL-3.0+",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Jonny Spitzner",
            "homepage": "https://github.com/jonnysp"
        }
    ],
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "php": "^5.6|^7.0"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Jonnysp\\Pannorama\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "contao-manager-plugin": "Jonnysp\\Pannorama\\ContaoManager\\Plugin"
    }
}
